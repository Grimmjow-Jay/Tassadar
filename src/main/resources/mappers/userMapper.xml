<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jay.tassadar.mapper.UserMapper">
    <insert id="signIn" parameterType="com.jay.tassadar.entity.User" useGeneratedKeys="true" keyProperty="userId">
        insert into t_user (user_name, phone) values (#{userName}, #{phone})
    </insert>
</mapper>